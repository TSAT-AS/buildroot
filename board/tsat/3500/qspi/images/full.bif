the_ROM_image:
{
	[pskfile] psk.pem
	[sskfile] ssk.pem
	[bootloader, authentication=rsa] fsbl.elf
	[authentication=rsa] fpga.bit
	[authentication=rsa] u-boot.elf
	[authentication=rsa, offset=0x490000, load=0x2000000] u-boot.dtb
	[offset=0x4C0000] u-boot-script.itb
	[offset=0x500000] uboot-env.bin
	[offset=0x5C0000] kernel-dtb.itb
	[offset=0xA40000] rootfs.squashfs
	[offset=0x2600000] appfs.jffs2
}
